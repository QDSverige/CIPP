{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.auth/*",
      "/_next/static/*",
      "/css/*",
      "/public/*",
      "/assets/*",
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml",
      "/manifest.json"
    ]
  },
  "responseOverrides": {
    "401": { "statusCode": 403 },
    "403": { "rewrite": "/403" },
    "404": { "rewrite": "/404" }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: blob: 'unsafe-eval' 'unsafe-inline'; object-src 'self' blob:; img-src 'self' blob: data: *"
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    { "route": "/.auth/*", "statusCode": 404 },
    { "route": "/*", "statusCode": 503 }
  ]
}
